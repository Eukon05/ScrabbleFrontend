<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="321.0" prefWidth="597.0" xmlns="http://javafx.com/javafx/21.0.7"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.ChatController">
   <TextArea fx:id="chatLog" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="254.0" prefWidth="566.0"/>
   <TextField fx:id="input" layoutX="14.0" layoutY="280.0" prefHeight="26.0" prefWidth="450.0"
              promptText="Type here..."/>
   <Button fx:id="send" layoutX="474.0" layoutY="281.0" mnemonicParsing="false" onAction="#sendMsg" prefHeight="26.0"
           prefWidth="108.0" text="Send!"/>
</AnchorPane>
