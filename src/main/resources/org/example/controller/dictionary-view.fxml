<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="92.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.7"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.DictionaryController">
   <TextField fx:id="input" layoutX="14.0" layoutY="23.0" prefHeight="26.0" prefWidth="502.0"
              promptText="Type in the word to check"/>
   <Label fx:id="result" layoutX="243.0" layoutY="59.0" prefHeight="18.0" prefWidth="114.0"/>
   <Button fx:id="checkBtn" layoutX="527.0" layoutY="23.0" mnemonicParsing="false" onAction="#onCheckBtn"
           text="Check!"/>
</AnchorPane>
